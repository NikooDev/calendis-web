    "@vis.gl/react-maplibre": "^8.1.0",
    "clsx": "^2.1.1",
    "firebase": "^12.4.0",
    "luxon": "^3.7.2",
    "maplibre-gl": "^5.10.0",
    "next": "^16.0.0",
    "rbush": "^4.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
